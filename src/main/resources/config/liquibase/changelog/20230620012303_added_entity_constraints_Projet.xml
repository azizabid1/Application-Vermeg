<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd">
    <!--
        Added the constraints for entity Projet.
    -->
    <changeSet id="20230620012303-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="equipe_id"
                                 baseTableName="projet"
                                 constraintName="fk_projet__equipe_id"
                                 referencedColumnNames="id"
                                 referencedTableName="equipe"/>

        <addForeignKeyConstraint baseColumnNames="projet_id"
                                 baseTableName="rel_projet__taches"
                                 constraintName="fk_rel_projet__taches__projet_id"
                                 referencedColumnNames="id"
                                 referencedTableName="projet"/>

        <addForeignKeyConstraint baseColumnNames="taches_id"
                                 baseTableName="rel_projet__taches"
                                 constraintName="fk_rel_projet__taches__taches_id"
                                 referencedColumnNames="id"
                                 referencedTableName="tache"/>
    </changeSet>
</databaseChangeLog>
